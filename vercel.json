{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "env": {
    "NEXT_PUBLIC_API_BASE_URL": "https://ai-insurance-platform-nkk3z2ndg-kingvis-projects.vercel.app",
    "NEXT_PUBLIC_APP_NAME": "AI Insurance Platform",
    "NEXT_PUBLIC_APP_VERSION": "1.0.0",
    "NEXT_PUBLIC_ENABLE_ANALYTICS": "false",
    "NEXT_PUBLIC_ENABLE_STAFF_DASHBOARD": "true",
    "NEXT_PUBLIC_ENABLE_BUSINESS_INTELLIGENCE": "true",
    "NEXT_PUBLIC_DEFAULT_CURRENCY": "USD",
    "NEXT_PUBLIC_DEFAULT_COUNTRY": "US",
    "NEXT_PUBLIC_DEBUG_MODE": "false",
    "NODE_ENV": "production",
    "BI_ENABLE_MOCK_DATA": "true",
    "BI_ENABLE_REAL_ANALYSIS": "false"
  },
  "build": {
    "env": {
      "NODE_ENV": "production",
      "NEXT_PUBLIC_SITE_URL": "https://ai-insurance-platform-nkk3z2ndg-kingvis-projects.vercel.app"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ],
  "functions": {
    "app/api/*/route.ts": {
      "maxDuration": 30
    }
  },
  "regions": ["iad1"],
  "github": {
    "silent": true
  }
} 